{
  "name": "game-ui",
  "version": "0.0.1",
  "description": "CRUD app for managing games",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "start": "fiori run --proxy http://localhost:4004 --open",
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "build:all": "cds build --clean && cds compile srv/ -2 edmx -l all -o gen/srv"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.17.4"
  }
}
