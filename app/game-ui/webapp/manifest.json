{
  "_version": "1.59.0",
  "sap.app": {
    "id": "gameui",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": ["en", "nl", "es"],
      "fallbackLocale": "en"
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.12.5",
      "toolsId": "ff4d5eca-0e35-45ba-a8eb-347a7f45f5f7"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/game/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReport|Student": {
            "controllerName": "gameui.ext.controller.StudentListExt"
          },
          "sap.fe.templates.ListReport.ListReport|Game": {
            "controllerName": "gameui.ext.controller.GameListExt"
          }
        }
      }
    },
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "gameui.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "StudentList",
          "target": "StudentList"
        },
        {
          "pattern": "Student({key}):?query:",
          "name": "StudentObjectPage",
          "target": "StudentObjectPage"
        },
        {
          "pattern": "Game",
          "name": "GameList",
          "target": "GameList"
        },
        {
          "pattern": "Game(Game_ID={Game_ID}):?query:",
          "name": "GameObjectPage",
          "target": "GameObjectPage"
        },
        {
          "pattern": "Game({key}):?query:",
          "name": "GameObjectPage",
          "target": "GameObjectPage"
        },
        {
          "pattern": "StudentFavoriteGamesView({key}):?query:",
          "name": "StudentFavoriteGameObjectPage",
          "target": "StudentFavoriteGameObjectPage"
        }
      ],
      "targets": {
        "StudentList": {
          "type": "Component",
          "id": "StudentList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Student",
              "variantManagement": "Page",
              "navigation": {
                "Student": {
                  "detail": {
                    "route": "StudentObjectPage"
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "GoToGames": {
                      "text": "Games",
                      "icon": "sap-icon://game",
                      "type": "Default",
                      "press": "onGoToGames"
                    }
                  }
                }
              }
            }
          }
        },
        "StudentObjectPage": {
          "type": "Component",
          "id": "StudentObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Student",
              "entityset": "Student",
              "navigation": {
                "to_favos/to_game": {
                  "detail": {
                    "route": "GameObjectPage"
                  }
                }
              }
            }
          }
        },
        "GameList": {
          "type": "Component",
          "id": "GameList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Game",
              "variantManagement": "Page",
              "navigation": {
                "Game": {
                  "detail": {
                    "route": "GameObjectPage"
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "GoToStudents": {
                      "text": "Students",
                      "icon": "sap-icon://employee",
                      "type": "Default",
                      "press": "onGoToStudents"
                    }
                  }
                }
              }
            }
          }
        },
        "GameObjectPage": {
          "type": "Component",
          "id": "GameObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Game",
              "contextPath": "/Game",
              "editableHeaderContent": false
            }
          }
        },
        "StudentFavoriteGameObjectPage": {
          "type": "Component",
          "id": "StudentFavoriteGameObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "contextPath": "/StudentFavoriteGamesView"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
